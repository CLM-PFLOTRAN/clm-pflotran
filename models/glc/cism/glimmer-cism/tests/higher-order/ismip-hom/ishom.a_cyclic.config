[ISMIP-HOM-TEST]

[parameters]
flow_factor = 1

[CF output]
variables = uvel vvel tau_xz tau_yz usurf thk topg
frequency = 0.01
name = ishom.a.out.nc

[grid]
upn = 11
ewn = 41    
nsn = 41
dew = 4000   
dns = 4000   
sigma_builtin = 1

[time]
tstart = 0.
tend = 0.03
dt = 0.01

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment A

[options]
dycore = 1              # SIA=0; 1st-order = 1
flow_law = 2            # temp dependent = 0; constant/uniform rate factor = 2
evolution = 3           # incremental remapping = 3
temperature = 0         # none = 0; incremental remapping = 3
 
[ho_options]
diagnostic_scheme = 1   # Payne/Price 1st-order dynamics = 1
which_ho_babc = 4       # no-slip basal boundary conditions = 4        
which_ho_efvs = 0       # nonlinear eff. visc. = 0 
which_ho_sparse = 4     # serial SLAP GMRES = 1; Trlinos = 4 
which_ho_nonlinear = 1  # Picard = 0; JFNK = 1

[CF input]
name = ishom.a.nc
time = 1
